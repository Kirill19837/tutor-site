{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "AWS": {
    "Profile": "eu2",
    "Region": "default",
    "AccessKey": "ACCESS_KEY",
    "SecretKey": "SECRET_KEY",
    "ServiceURL": "https://eu2.contabostorage.com/af85d0c911b141f59483acc3a958d4d1:tutorpro.pl",
    "DisableHostPrefixInjection": true
  },
  "Umbraco": {
    "CMS": {
      "ModelsBuilder": {
        "ModelsDirectory": "~/TutorPro.Domain/Models/Generated",
        "Enable": true,
        "ModelsMode": "SourceCodeManual"
      },
      "Examine": {
        "LuceneDirectoryFactory": "TempFileSystemDirectoryFactory" //if you need to know more information about this https://docs.umbraco.com/umbraco-cms/fundamentals/setup/server-setup/load-balancing/file-system-replication
      },
      "Global": {
        "UseHttps": false,
        "SanitizeTinyMce": true,
        "Smtp": {
          "Username": "test@gmail.com",
          "Password": "test@gmail.com",
          "From": "test@gmail.com",
          "Host": "smtp.gmail.com",
          "Port": 587
        }
      },
      "Hosting": {
        "LocalTempStorageLocation": "EnvironmentTemp",
        "Debug": true
      },
      "Content": {
        "MacroErrors": "Throw"
      }
    },
    "Storage": {
      "AWSS3": {
        "Media": {
          "BucketHostName": "contabostorage",
          "BucketName": "tutorpro.pl"
        }
      }
    }
  }
}