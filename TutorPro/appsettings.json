{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "ModelsBuilder": {
        "ModelsDirectory": "~/../TutorPro.Domain/Models/Generated", //if you want to use docker, you will need this ~/TutorPro.Domain/Models/Generated locally ~/../TutorPro.Domain/Models/Generated
        "Enable": true,
        "ModelsMode": "SourceCodeManual"
      },
      "Global": {
        "Id": "cbfe474d-de1c-47bd-9e83-61e5576da183",
        "UseHttps": true,
        "SanitizeTinyMce": true,
        "Smtp": {
          "Username": "test@gmail.com",
          "Password": "test@gmail.com",
          "From": "test@gmail.com",
          "Host": "smtp.gmail.com",
          "Port": 587
        }
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "Error404Collection": [
          {
            "Culture": "default",
            "ContentKey": "94c52261-d2b3-4041-9eb2-01eeaf15c18f"
          }
        ]
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True;",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite",
    "DefaultConnection": "Data Source=Umbraco.sqlite.db"
  }
}