{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "ModelsBuilder": {
        "ModelsDirectory": "~/TutorPro.Domain/Models/Generated", //if you run locally use ~/../TutorPro.Domain/Models/Generated
        "Enable": true,
        "ModelsMode": "SourceCodeManual"
      },
      "Global": {
        "Id": "cbfe474d-de1c-47bd-9e83-61e5576da183",
        "UseHttps": true,
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=sqlserver;Database=TutorProUmbraco;User Id=sa;Password=YourStrong@Passw0rd;TrustServerCertificate=true;", //if you run locally you can use localhost,1433 instead of sqlserver
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  }
}