    {
      "contentTypeKey": "54ae3370-05ef-4ce3-be63-83831613a7cb",
      "udi": "umb://element/3104a258f69a4848b2e0ffb047bc826d",
      "tTitle": "Воронка продажів",
      "tBold": "0",
      "tSize": "",
