{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "AWS": {
    "Profile": "eu2",
    "ProfilesLocation": "eu2",
    "Region": "default",
    "AccessKey": "ACCESSKEY_AWS",
    "SecretKey": "SECRETKEY",
    "ServiceURL": "https://eu2.contabostorage.com/af85d0c911b141f59483acc3a958d4d1:tutorpro.pl",
    "DisableHostPrefixInjection": true
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw"
      },
      "Hosting": {
        "Debug": true
      },
      "Global": {
        "UseHttps": false
      },
      "RuntimeMinification": {
        "UseInMemoryCache": true,
        "CacheBuster": "Timestamp"
      }
    },
    "Storage": {
      "AWSS3": {
        "Media": {
          "BucketHostName": "contabostorage",
          "BucketName": "tutorpro.pl"
        }
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  }
}
