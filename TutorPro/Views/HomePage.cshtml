@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<HomePage>
@{
	Layout = "Main.cshtml";
}

@if (Model.TContentPage is TBlogGridPage blockGridPage)
{
    <div>
        @await Html.PartialAsync("BlockGridPage", blockGridPage)
    </div>
}

@if (Model.TContentPage is TListPage listPage)
{
    <div>
        @await Html.PartialAsync("ListPage", listPage)
    </div>
}