@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<HomePage>

<footer class="footer radial-bg">
    <div class="container">
        <div class="footer__inner">
            @if(Model != null)
            {
                @if(Model.TAboutFoot != null)
                {
                    @await Html.PartialAsync("tutorPartials/TAbout", Model.TAboutFoot.Content)                  
                }
                @if(Model.TQuickLinks != null)
                {
                    @await Html.PartialAsync("tutorPartials/TQuickLink", Model.TQuickLinks.Content)                  
                }
                @if(Model.TContactUsFooter != null)
                {
                    @await Html.PartialAsync("tutorPartials/TContacts", Model.TContactUsFooter.Content)
                }
            }                
        </div>
    </div>
</footer>
