@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.MaterialPage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
    Layout = "Main.cshtml";   
}

@if(Model.TInnerTop != null)
{
    @await Html.GetBlockGridHtmlAsync(Model.TInnerTop);
}
@if(Model.TMaterialFilters != null)
{
    @await Html.GetBlockGridHtmlAsync(Model.TMaterialFilters); 
}
@if(Model.TAction != null)
{
    @await Html.GetBlockGridHtmlAsync(Model.TAction); 
}
